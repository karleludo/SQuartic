        <form id="form" action="" method="POST">
=======
        <form id="form" action="page2" method="POST">

            <div class="progresssbar-container">

            </div>

            <input type="text" name="name" id="name" placeholder="Your Name" required>
            <select id="course" name="Course" required>
                <option selected="selected" disabled>Select a Course</option>
                <option value="AB Communication">AB Communication</option>
                <option value="AB Foreign Service">AB Foreign Service</option>
                <option value="AB Legal Studies">AB Legal Studies</option>
                <option value="Bachelor in Secondary Education major in English">Bachelor in Secondary Education major
                    in English </option>
                <option value="Bachelor of Arts in Multimedia Arts">Bachelor of Arts in Multimedia Arts </option>
                <option value="Bachelor of Elementary Education">Bachelor of Elementary Education </option>
                <option value="BS Accountancy">BS Accountancy </option>
                <option value="BS Architecture">BS Architecture </option>
                <option value="BS Biology">BS Biology </option>
                <option value="BS Business Administration major in: Management Accounting">BS Business Administration
                    major in: Management Accounting </option>
                <option value="BS Business Administration major in: Operations Management">BS Business Administration
                    major in: Operations Management </option>
                <option value="BS Business Administration major in: Human Resource Development Management">BS Business
                    Administration major in: Human Resource Development Management </option>
                <option value="BS Business Administration major in: Marketing Management">BS Business Administration
                    major in: Marketing Management </option>
                <option value="BS Civil Engineering">BS Civil Engineering </option>
                <option value="BS Computer Engineering">BS Computer Engineering </option>
                <option value="BS Computer Science">BS Computer Science </option>
                <option value="BS Customs Administration">BS Customs Administration </option>
                <option value="BS Electrical Engineering">BS Electrical Engineering </option>
                <option value="BS Electronics and communication Engineering">BS Electronics and communication
                    Engineering </option>
                <option value="BS Industrial Engineering">BS Industrial Engineering </option>
                <option value="BS Information Technology">BS Information Technology </option>
                <option
                    value="BS International Hospital Management with Specialization in: Cruise Line Operations in Culinary Arts">
                    BS International Hospital Management with Specialization in: Cruise Line Operations in Culinary Arts
                </option>
                <option
                    value="BS International Hospital Management with Specialization in: Cruise Line Operations in Hotel Services">
                    BS International Hospital Management with Specialization in: Cruise Line Operations in Hotel
                    Services </option>
                <option
                    value="BS International Hospital Management with Specialization in: Culinary Arts and Kitchen Operations">
                    BS International Hospital Management with Specialization in: Culinary Arts and Kitchen Operations
                </option>
                <option
                    value="BS International Hospital Management with Specialization in: Hotel and Restaurant Administration">
                    BS International Hospital Management with Specialization in: Hotel and Restaurant Administration
                </option>
                <option value="BS Internation Travel And Tourism Management">BS Internation Travel And Tourism
                    Management </option>
                <option
                    value="BS Internation Travel And Tourism Management with specialization in: Health and Wellness">BS
                    Internation Travel And Tourism Management with specialization in: Health and Wellness </option>
                <option value="BS Mechanical Engineering">BS Mechanical Engineering </option>
                <option value="BS Medical Technology">BS Medical Technology </option>
                <option value="BS Pharmacy">BS Pharmacy </option>
                <option value="BS Psychology">BS Psychology </option>
                <option value="BS Radiolic Technology">BS Radiolic Technology </option>

            </select>

            <input type="date" name="date" id="date" disabled>
            <input type="text" name="nationality" id="nationality" placeholder="nationality" required>

            <div class="radio-btn">
                <input type="radio" name="gender" id="male" value="male" required>
                <label for="male">
                    <i class="fa fa-male" aria-hidden="true"></i>
                    <span>Male</span>
                </label>
                <input type="radio" name="gender" id="female" value="female" required>
                <label for="female">
                    <i class="fa fa-female" aria-hidden="true"></i>
                    <span>Female</span>
                </label>
            </div>
            
            <input type="number" name="contact-num" id="contact-num" min="0" max="99999999999" placeholder="Number" required>
            <input type="email" name="email" id="email" placeholder="Email Address" required>

            <input type="submit" value="Next" id="cta-next">

        </form>
